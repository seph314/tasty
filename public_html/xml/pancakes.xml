<cookbook version="46">
    <recipe>
        <title>Buckwheat Pancakes</title>
        <ingredient>
            <li>1 banana.</li>
            <li>2 dl buckwheat flour.</li>
            <li>~4 dl water.</li>
        </ingredient>
        <recipetext>
            <li>Mix all the ingredients in a bowl.</li>
            <li>Fry in frying pan one scoop at a time.</li>
            <li>Flip after ca 2-4 minutes.</li>
        </recipetext>
        <imagepath>/~ocean/seminar/public_html/img/content/pancakes.jpg</imagepath>
    </recipe>

</cookbook>